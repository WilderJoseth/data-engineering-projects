					}
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "DROP VIEW IF EXISTS silver.vw_Customer_Clean;"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "Create view silver cleaning",
				"type": "Script",
				"dependsOn": [
					{
						"activity": "Drop view silver cleaning",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "SqlServer_Serverless_Pool",
					"type": "LinkedServiceReference",
					"parameters": {
						"DBName": "AdventureWorksLT2022_DB"
					}
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "CREATE VIEW silver.vw_Customer_Clean\r\nAS\r\nSELECT\r\n\tCustomerID,\r\n\tNameStyle,\r\n\tUPPER(TRIM(ISNULL(Title, ''))) AS Title,\r\n\tUPPER(TRIM(FirstName)) AS FirstName,\r\n\tUPPER(TRIM(ISNULL(MiddleName, ''))) AS MiddleName,\r\n\tUPPER(TRIM(LastName)) AS LastName,\r\n\tUPPER(TRIM(ISNULL(Suffix, ''))) AS Suffix,\r\n\tUPPER(TRIM(ISNULL(CompanyName, ''))) AS CompanyName,\r\n\tUPPER(TRIM(ISNULL(SalesPerson, ''))) AS SalesPerson,\r\n\tUPPER(TRIM(ISNULL(EmailAddress, ''))) AS EmailAddress,\r\n\tUPPER(TRIM(ISNULL(Phone, ''))) AS Phone\r\nFROM bronze.Customer;"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "Materialize clean data",
				"type": "Script",
