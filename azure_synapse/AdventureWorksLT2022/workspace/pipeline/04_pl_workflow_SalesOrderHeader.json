				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "IF (SELECT OBJECT_ID('bronze.SalesOrderHeader', 'U')) IS NOT NULL\r\n    DROP EXTERNAL TABLE bronze.SalesOrderHeader;\r\n\r\nCREATE EXTERNAL TABLE bronze.SalesOrderHeader\r\n(\r\n    SalesOrderID INT,\r\n    RevisionNumber TINYINT,\r\n    OrderDate DATETIME,\r\n    DueDate DATETIME,\r\n    ShipDate DATETIME,\r\n    Status TINYINT,\r\n    OnlineOrderFlag BIT,\r\n    SalesOrderNumber NVARCHAR(25),\r\n    PurchaseOrderNumber NVARCHAR(25),\r\n    AccountNumber NVARCHAR(15),\r\n    CustomerID INT,\r\n    ShipToAddressID INT,\r\n    BillToAddressID INT,\r\n    ShipMethod NVARCHAR(50),\r\n    CreditCardApprovalCode VARCHAR(25),\r\n    SubTotal MONEY,\r\n    TaxAmt MONEY,\r\n    Freight MONEY,\r\n    TotalDue MONEY,\r\n    Comment NVARCHAR(MAX),\r\n    rowguid UNIQUEIDENTIFIER,\r\n    ModifiedDate DATETIME\r\n)\r\nWITH\r\n(\r\n    DATA_SOURCE = bronze_data,\r\n    LOCATION = 'SalesOrderHeader/*.parquet',\r\n    FILE_FORMAT = ParquetFormat\r\n);"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
