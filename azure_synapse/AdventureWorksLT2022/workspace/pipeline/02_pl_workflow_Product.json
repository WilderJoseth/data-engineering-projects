			{
				"name": "Create table bronze",
				"type": "Script",
				"dependsOn": [
					{
						"activity": "Create raw file",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "SqlServer_Serverless_Pool",
					"type": "LinkedServiceReference",
					"parameters": {
						"DBName": "AdventureWorksLT2022_DB"
					}
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "IF (SELECT OBJECT_ID('bronze.Product', 'U')) IS NOT NULL\r\n    DROP EXTERNAL TABLE bronze.Product;\r\n\r\nCREATE EXTERNAL TABLE bronze.Product\r\n(\r\n    ProductID INT,\r\n    Name NVARCHAR(50),\r\n    ProductNumber NVARCHAR(25),\r\n    Color NVARCHAR(15),\r\n    StandardCost MONEY,\r\n    ListPrice MONEY,\r\n    Size NVARCHAR(5),\r\n    Weight DECIMAL(8, 2),\r\n    ProductCategoryID INT,\r\n    ProductModelID INT,\r\n    SellStartDate DATETIME,\r\n    SellEndDate DATETIME,\r\n    DiscontinuedDate DATETIME,\r\n    ThumbNailPhoto VARBINARY(MAX),\r\n    ThumbnailPhotoFileName NVARCHAR(50),\r\n    rowguid UNIQUEIDENTIFIER,\r\n    ModifiedDate DATETIME\r\n)\r\nWITH\r\n(\r\n    DATA_SOURCE = bronze_data,\r\n    LOCATION = 'Product/*.parquet',\r\n    FILE_FORMAT = ParquetFormat\r\n);"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "Drop view silver cleaning",
				"type": "Script",
				"dependsOn": [
