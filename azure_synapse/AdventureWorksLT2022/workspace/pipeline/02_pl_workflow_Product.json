				}
			},
			{
				"name": "Create view silver cleaning",
				"type": "Script",
				"dependsOn": [
					{
						"activity": "Drop view silver cleaning",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "SqlServer_Serverless_Pool",
					"type": "LinkedServiceReference",
					"parameters": {
						"DBName": "AdventureWorksLT2022_DB"
					}
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "CREATE VIEW silver.vw_Product_Clean\r\nAS\r\nSELECT\r\n\tProductID,\r\n\tUPPER(TRIM(Name)) AS Name,\r\n\tUPPER(TRIM(ProductNumber)) AS ProductNumber,\r\n\tUPPER(TRIM(ISNULL(Color, ''))) AS Color,\r\n\tCAST(StandardCost AS DECIMAL(10, 4)) AS StandardCost,\r\n\tCAST(ListPrice AS DECIMAL(10, 4)) AS ListPrice,\r\n\tUPPER(TRIM(ISNULL(Size, ''))) AS Size,\r\n\tISNULL(Weight, 0) AS Weight,\r\n\tISNULL(ProductCategoryID, 0) AS ProductCategoryID,\r\n\tISNULL(ProductModelID, 0) AS ProductModelID,\r\n\tSellStartDate,\r\n\tISNULL(SellEndDate, CAST('1900-01-01' AS DATETIME)) AS SellEndDate,\r\n\tISNULL(DiscontinuedDate, CAST('1900-01-01' AS DATETIME)) AS DiscontinuedDate,\r\n\tUPPER(TRIM(ISNULL(ThumbnailPhotoFileName, ''))) AS ThumbnailPhotoFileName\r\nFROM bronze.Product;"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
