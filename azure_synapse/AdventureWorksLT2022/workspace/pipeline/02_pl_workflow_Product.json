				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "IF (SELECT OBJECT_ID('silver.Product', 'U')) IS NOT NULL\r\n    DROP EXTERNAL TABLE silver.Product;\r\n\r\nCREATE EXTERNAL TABLE silver.Product\r\n    WITH (\r\n        LOCATION = 'Product/',\r\n        DATA_SOURCE = silver_data,\r\n        FILE_FORMAT = ParquetFormat\r\n    )\r\nAS\r\nSELECT\r\n\tProductID,\r\n\tName,\r\n\tProductNumber,\r\n\tColor,\r\n\tStandardCost,\r\n\tListPrice,\r\n\tSize,\r\n\tWeight,\r\n\tProductCategoryID,\r\n\tProductModelID,\r\n\tSellStartDate,\r\n\tSellEndDate,\r\n\tDiscontinuedDate,\r\n\tThumbnailPhotoFileName\r\nFROM silver.vw_Product_Clean;"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
