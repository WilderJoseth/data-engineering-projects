					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "SqlServer_Serverless_Pool",
					"type": "LinkedServiceReference",
					"parameters": {
						"DBName": {
							"value": "@pipeline().parameters.BD",
							"type": "Expression"
						}
					}
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "CREATE VIEW silver.vw_ProductModel_Clean\r\nAS\r\nSELECT\r\n\tProductModelID,\r\n\tUPPER(TRIM(Name)) AS Name\r\nFROM bronze.ProductModel;"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "Materialize clean data",
				"type": "Script",
				"dependsOn": [
